


class Solution{
    int middle(int A, int B, int C){
        //code here
        if(A > B && A > C){
            if(B > C) return B;
            else return C;
        }
        else if(B > A && B > C){
            if(A > C) return A;
            else return C;
        }
        else{
            return B;   
        }
    }
}
